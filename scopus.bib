@ARTICLE{Munteanu2017123,
author={Munteanu, A.M. and Stocker, M. and Stöwe, M. and Massen, J.J.M. and Bugnyar, T.},
title={Behavioural and Hormonal Stress Responses to Social Separation in Ravens, Corvus corax},
journal={Ethology},
